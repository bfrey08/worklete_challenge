Quiz #:<p id=quiz_id><%= @quiz.id%></p>
<br>
<% @quiz_questions.each_with_index do |quiz_question,index| %>
<div class=<%="question#{index + 1}"%>>
<%= "Question #{index + 1}:"%>
<%= quiz_question.question.text %>
<%= button_to 'true', "/learners/#{params[:learner_id]}/quizzes/#{@quiz.id}", params: {quiz_question_id: quiz_question.id, learner_answer: "true"}, class: "btn btn-secondary", method: :patch %><br>
<%= button_to 'false', "/learners/#{params[:learner_id]}/quizzes/#{@quiz.id}", params: {quiz_question_id: quiz_question.id, learner_answer: "false"}, class: "btn btn-secondary", method: :patch %><br>
</div>
<br>
<% end %>
<p id="manager_id"> <%= @quiz.manager_id %> </p>
<body data-topic-id="<%= @quiz.manager_id %>">
